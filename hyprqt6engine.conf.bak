# HyprQt6Engine Configuration
# Neon Cyberpunk Theme

[General]
theme=neon
follow_hyprland=true

[Colors]
# Neon color scheme
accent=#FF00FF
background=#0A0E14
foreground=#E6E6E6
highlight=#00FFFF

[Style]
# Enable neon effects
glow_enabled=true
transparency=0.95
border_radius=8

[Performance]
# RTX 4090 optimizations
gpu_acceleration=true
vsync=true
